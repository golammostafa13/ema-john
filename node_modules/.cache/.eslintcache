       </div>\n            <div className=\"shop-cart\">\n                <Cart cart={cart}>\n                    <Link to=\"/review\">\n                        <button className=\"main-button\">Review Order</button>\n                    </Link>\n                </Cart>\n            </div>\n        </div>\n    );\n};\n\nexport default Shop;","/home/mostafa/Documents/ema-john/ema-john/src/components/Review/Review.js",["88"],"import React, { useEffect, useState } from 'react';\nimport { useHistory } from 'react-router';\nimport fakeData from '../../fakeData';\nimport orderImg from '../../images/giphy.gif';\nimport { getDatabaseCart, removeFromDatabaseCart } from '../../utilities/databaseManager';\nimport Cart from '../Cart/Cart';\nimport ReviewItem from '../ReviewItem/ReviewItem';\nconst Review = () => {\n    const [cart, setCart] = useState([]);\n    const removeProduct = productKey =>{\n        const newCart = cart.filter(product => product.key !== productKey);\n        setCart(newCart);\n        removeFromDatabaseCart(productKey);\n    }\n    const [happyImg, setHappyImg] = useState(false);\n    let history = useHistory();\n    const handleProccessOrder = () => {\n        history.push('/shipment');\n    }\n    let thankYou;\n    if(happyImg){\n        thankYou = <img src={orderImg} alt=\"\"/>\n    }\n    useEffect(() => {\n        const saveCart = getDatabaseCart();\n        const productKeys = Object.keys(saveCart);\n\n        const newCart = productKeys.map(key => {\n            const product = fakeData.find(pd => pd.key === key);\n            product.quantity = saveCart[key];\n            return product;\n        });\n        setCart(newCart)\n    }, []);\n    return (\n        <div className=\"shop-review\">\n            <div className=\"shop-product\">\n                <h1>Items Order: {cart.length}</h1>\n                {\n                    cart.map(pd => <ReviewItem removeProduct = {removeProduct} key={pd.key} product = {pd}></ReviewItem>)\n                }\n                {\n                    thankYou\n                }\n            </div>\n            <div className=\"shop-cart\">\n                <Cart cart={cart}>\n                    <button onClick={handleProccessOrder} className=\"main-button\">Process Order</button>\n                </Cart>\n            </div>\n        </div>\n    );\n};\n\nexport default Review;","/home/mostafa/Documents/ema-john/ema-john/src/components/ProductDetail/ProductDetail.js",[],"/home/mostafa/Documents/ema-john/ema-john/src/components/Cart/Cart.js",[],"/home/mostafa/Documents/ema-john/ema-john/src/components/Products/Products.js",[],"/home/mostafa/Documents/ema-john/ema-john/src/utilities/databaseManager.js",[],"/home/mostafa/Documents/ema-john/ema-john/src/components/ReviewItem/ReviewItem.js",[],"/home/mostafa/Documents/ema-john/ema-john/src/fakeData/index.js",[],"/home/mostafa/Documents/ema-john/ema-john/src/fakeData/android.js",[],"/home/mostafa/Documents/ema-john/ema-john/src/fakeData/camera.js",[],"/home/mostafa/Documents/ema-john/ema-john/src/fakeData/laptop.js",[],"/home/mostafa/Documents/ema-john/ema-john/src/components/Login/Login.js",["89"],"/home/mostafa/Documents/ema-john/ema-john/src/components/Shipment/Shipment.js",[],"/home/mostafa/Documents/ema-john/ema-john/src/components/Cart/FirebaseConfig.js",[],{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","severity":1,"message":"95","line":10,"column":22,"nodeType":"96","messageId":"97","endLine":10,"endColumn":33},{"ruleId":"94","severity":1,"message":"98","line":15,"column":22,"nodeType":"96","messageId":"97","endLine":15,"endColumn":33},{"ruleId":"94","severity":1,"message":"99","line":22,"column":10,"nodeType":"96","messageId":"97","endLine":22,"endColumn":22},"no-native-reassign",["100"],"no-negated-in-lhs",["101"],"no-unused-vars","'setProducts' is assigned a value but never used.","Identifier","unusedVar","'setHappyImg' is assigned a value but never used.","'loggedInUser' is assi